{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@env/*": [
        "src/environments/*"
      ],
      "@app/core": [
        "src/app/core/public_api"
      ],
      "@app/shared": [
        "src/app/shared/public_api"
      ],
      "@app/feature/base-ms": [
        "src/app/feature/base-ms/public_api"
      ],
      "@app/feature/basic-ms": [
        "src/app/feature/basic-ms/public_api"
      ],
      "@app/feature/basic-aux": [
        "src/app/feature/basic-aux/public_api"
      ],
      "@app/feature/designer-ms": [
        "src/app/feature/designer-ms/public_api"
      ],
      "@app/feature/morejee-ms": [
        "src/app/feature/morejee-ms/public_api"
      ],
      "@app/feature/morejee-aux": [
        "src/app/feature/morejee-aux/public_api"
      ],
      "@app/feature/oss-ms": [
        "src/app/feature/oss-ms/public_api"
      ],
      "@app/feature/oms-ms": [
        "src/app/feature/oms-ms/public_api"
      ],
      "@app/feature/oms-aux": [
        "src/app/feature/oms-aux/public_api"
      ],
      "app-basic": [
        "dist/app-basic"
      ],
      "app-basic/*": [
        "dist/app-basic/*"
      ],
      "app-oms": [
        "dist/app-oms"
      ],
      "app-oms/*": [
        "dist/app-oms/*"
      ],
      "app-morejee": [
        "dist/app-morejee"
      ],
      "app-morejee/*": [
        "dist/app-morejee/*"
      ],
      "app-oss": [
        "dist/app-oss"
      ],
      "app-oss/*": [
        "dist/app-oss/*"
      ]
    }
  }
}