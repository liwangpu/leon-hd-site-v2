{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@env/*": [
        "src/environments/*"
      ],
      "@app/core": [
        "src/app/core/public_api"
      ],
      "app-basic1": [
        "projects/app-basic/src/public-api.ts"
      ],
      "app-oms": [
        "projects/app-oms/src/public-api.ts"
      ],
      "app-morejee": [
        "dist/app-morejee"
      ],
      "app-morejee/*": [
        "dist/app-morejee/*"
      ],
      "app-oss": [
        "dist/app-oss"
      ],
      "app-oss/*": [
        "dist/app-oss/*"
      ],
      "app-common1": [
        "projects/app-common/src/public-api.ts"
      ]
    }
  }
}